{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "json": {
      "format": "%(message)s"
    },
    "plain": {
      "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "json",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "server": {
      "handlers": ["console"],
      "level": "INFO",
      "propagate": false
    }
  },
  "root": {
    "handlers": ["console"],
    "level": "INFO"
  }
}